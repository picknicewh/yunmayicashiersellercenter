<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--corner_color 边角颜色，
      scanner_color 扫描线颜色
      possible_result_color 扫描点颜色
      frame_color 扫描框边线颜色
      mask_color 模糊区域颜色
      label_text  框上方提示
  -->
    <declare-styleable name="ViewfinderView">
        <attr name="corner_color" format="color"/>
        <attr name="laser_color" format="color"/>
        <attr name="frame_color" format="color" />
        <attr name="mask_color" format="color" />
        <attr name="result_point_color" format="color"/>
        <attr name="result_color" format="color" />
        <attr name="label_text_color" format="color"/>
        <attr name="label_text" format="string"/>
        <attr name="label_text_size" format="float"/>
    </declare-styleable>
     <declare-styleable name="ItemBase">
         <attr name="label" format="string"/>
         <attr name="hitText" format="string"/>
         <attr name="hitTextColor" format="color"/>
         <attr name="mEditTextColor" format="color"/>
         <attr name="mBackgroundColor" format="color"/>
         <attr name="mInputType" format="string"/>
         <attr name="mMaxLength" format="integer"/>

     </declare-styleable>
    <declare-styleable name="ItemTextView">
        <attr name="mLabelTextColor" format="color"/>
        <attr name="mHintTextColor" format="color"/>
       <attr name="mLabel" format="string"/>
        <attr name="mHint" format="string"/>
        <attr name="isSmall" format="boolean"/>
    </declare-styleable>
</resources>